{
  "name": "react-phonegap-onsenui",
  "version": "1.0.0",
  "description": "A React Phonegap Onsen UI boilerplate",
  "main": "src/index.js",
  "scripts": {
    "android": "cordova run android",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "ios": "cordova run ios",
    "clean": "rimraf www/*",
    "hot": "npm run clean && webpack --env=dev --watch",
    "hot:prod": "npm run clean && webpack --env=prod --watch",
    "prepare": "npm run clean && node config && webpack --env=prod && cordova prepare",
    "build": "npm run clean && node config && webpack --env=prod && cordova build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/*/RbGenerated*/*.{js,jsx}",
      "!src/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/supports/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/supports/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/supports/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "bootify": "^1.0.0",
    "chalk": "^2.4.2",
    "compression": "^1.6.2",
    "cross-env": "^5.2.1",
    "fontfaceobserver": "^2.0.9",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "invariant": "^2.2.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.2",
    "loglevel": "^1.4.1",
    "minimist": "^1.2.0",
    "onsenui": "^2.2.1",
    "react": "^16.9.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.0.2",
    "react-intl": "^3.2.0",
    "react-onsenui": "^1.2.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-tappable": "^1.0.4",
    "react-with-context": "^2.0.0",
    "redux": "^4.0.4",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.3.2",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-react-intl": "^4.1.16",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-promise-to-bluebird": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^4.2.0",
    "cheerio": "^0.22.0",
